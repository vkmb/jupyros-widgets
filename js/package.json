{
  "name": "jupyros-widgets",
  "version": "0.1.0",
  "description": "Implemention of jupyros",
  "author": "vkmb",
  "license": "BSD-3-Clause",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com//jupyros-widgets.git"
  },
  "keywords": [
    "jupyter",
    "widgets",
    "ipython",
    "ipywidgets",
    "jupyterlab-extension"
  ],
  "files": [
    "lib/**/*.js",
    "dist/*.js"
  ],
  "scripts": {
    "clean": "rimraf dist/ && rimraf ../jupyros_widgets/labextension/ && rimraf ../jupyros_widgets/nbextension",
    "prepublish": "yarn run clean && yarn run build:prod",
    "build": "webpack --mode=development && yarn run build:labextension:dev",
    "build:prod": "webpack --mode=production && yarn run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "watch": "webpack --watch --mode=development",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.0.0",
    "rimraf": "^2.6.1",
    "webpack": "^5",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^1.1 || ^2 || ^3 || ^4 || ^6",
    "@lumino/coreutils": "^1.12.1",
    "bufferutil": "^4.0.7",
    "crypto": "^1.0.1",
    "encoding": "^0.1.13",
    "node-fetch": "^2.6.11",
    "roslib": "^1.3.0",
    "utf-8-validate": "^6.0.3",
    "ws": "^8.13.0"
  },
  "jupyterlab": {
    "extension": "lib/labplugin",
    "outputDir": "../jupyros_widgets/labextension",
    "sharedPackages": {
      "@jupyter-widgets/base": {
        "bundled": false,
        "singleton": true
      }
    }
  }
}
